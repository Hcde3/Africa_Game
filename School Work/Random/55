import random
again = 1
num = random.randint(1,5)
ans = int(input("Guess a number from 1 to 5\n"))
if ans == num:
    print("Well done")
    again = 0
elif ans > num:
    print("Too high")
elif ans < num:
    print("Too low")
if again == 1:
    ans = int(input("Guess another number from 1 to 5\n"))
    if ans == num:
        print("Well done")
        quit
    else:
        print("You loose")
